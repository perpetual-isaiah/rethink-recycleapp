{
  "expo": {
    "name": "MyRecyclingApp",
    "slug": "myrecyclingapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.CAMERA"
      ],
      "package": "com.perpetual.isaiah.myrecyclingapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-barcode-scanner",
      "expo-build-properties"
    ],
    "extra": {
      "cloudinaryCloudName": "dolg3dv2x",
      "cloudinaryUploadPreset": "unsigned_profile",
      "eas": {
        "projectId": "d94c685c-a17b-48a0-a1b9-71fa1b30cd7b"
      }
    },
    "owner": "perpetual.isaiah"
  }
}
